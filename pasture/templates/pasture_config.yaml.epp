<%- | $port,
      $default_character,
      $default_message,
      $sinatra_server,
      $db,
| -%>
# This file is managed by Puppet. Please do not make manual changes.
---
:default_character: <%= $default_character %>
:default_message:   <%= $default_message %>
<%- if $db { -%>
 :db: <%= $db %>
<%- } %>
:sinatra_settings:
  :port: <%= $port %>
  :server: <%= $sinatra_server %>
